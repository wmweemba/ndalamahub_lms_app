      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      },
      "jsx": "react-jsx",
      "allowSyntheticDefaultImports": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }